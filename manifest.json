{
  "manifest_version": 3,
  "name": "Processador de Clipboard",
  "version": "1.0",
  "description": "Captura os dados do clipboard e conta os registros totais e Ãºnicos.",
  "permissions": [
    "clipboardRead",
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
